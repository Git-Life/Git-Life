{
  "name": "git-life",
  "version": "1.0.0",
  "description": "An effective way to find and contribute to repos you're interested in",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "webpack-prod": "NODE_ENV=production webpack --progress -p",
    "webpack-dev": "NODE_ENV=development webpack --progress",
    "server-prod": "node ./index.js",
    "server-dev": "nodemon ./index.js",
    "start": "npm run clean && npm run webpack-prod && npm run server-prod",
    "dev": "npm run clean && npm run webpack-dev && npm run server-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Git-Life/Git-Life.git"
  },
  "author": "Jenna Gain, Sheryl Hohman, Alex Paczynski, Caleb Rogers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Git-Life/Git-Life/issues"
  },
  "homepage": "https://github.com/Git-Life/Git-Life#readme",
  "dependencies": {
    "axios": "^0.10.0",
    "babel-preset-react-hmre": "^1.1.1",
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "html-webpack-plugin": "^2.16.0",
    "lodash": "^4.11.1",
    "path": "^0.12.7",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "webpack": "^1.13.0",
    "webpack-hot-middleware": "^2.10.0"
  }
}
