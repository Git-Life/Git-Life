{
  "name": "git-life",
  "version": "1.0.0",
  "description": "An effective way to find and contribute to repos you're interested in",
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
    "clean": "rm -rf dist",
    "webpack-prod": "NODE_ENV=production webpack --progress -p",
    "webpack-dev": "NODE_ENV=development webpack --progress",
    "server-prod": "node ./index.js",
    "server-dev": "nodemon ./index.js",
    "prestart": "npm run clean && npm run webpack-prod",
    "start": "npm run server-prod",
    "dev": "npm run clean && npm run webpack-dev && npm run server-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Git-Life/Git-Life.git"
  },
  "author": "Jenna Gain, Sheryl Hohman, Alex Paczynski, Caleb Rogers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Git-Life/Git-Life/issues"
  },
  "homepage": "https://github.com/Git-Life/Git-Life#readme",
  "dependencies": {
    "axios": "^0.10.0",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "html-webpack-plugin": "^2.16.0",
    "lodash": "^4.11.1",
    "path": "^0.12.7",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "request": "^2.72.0",
    "webpack": "^1.13.0",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-preset-airbnb": "^1.0.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^5.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.7",
